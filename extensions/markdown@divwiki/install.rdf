<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>markdown@divwiki</em:id>
    <em:version>0.1</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>false</em:unpack>
    <!-- DivWiki -->
    <em:targetApplication>
      <Description>
        <em:id>divwiki@divwiki</em:id>
        <em:minVersion>0.1</em:minVersion>
        <em:maxVersion>2.0</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>Markdown</em:name>
    <em:description>Markdown wiki provider for DivWiki</em:description>
    <em:creator>Adrian Arroyo Calle</em:creator>
	<em:developer>Adrian Arroyo Calle</em:developer>
	<em:contributor>Adrian Arroyo Calle</em:contributor>
	<em:translator>Adrian Arroyo Calle</em:translator>
    <em:homepageURL>http://github.com/AdrianArroyoCalle/divwiki</em:homepageURL>
	<em:updateURL>https://raw2.github.com/AdrianArroyoCalle/divwiki/master/extensions/markdown@divwiki/install.rdf</em:updateURL>
	<em:aboutURL>chrome://markdown/content/about.xul</em:aboutURL>
	<em:optionsURL>chrome://markdown/content/options.xul</em:optionsURL>
	<em:optionsType>2</em:optionsType>
	<em:iconURL>chrome://markdown/content/48x30-solid.png</em:iconURL>
	<em:icon64URL>chrome://markdown/content/66x40-solid.png</em:icon64URL>
	<em:localized>
		<Description>
			<em:locale>es-ES</em:locale>
			<em:name>Markdown</em:name>
			<em:description>Proveedor de Wikis Markdown para DivWiki</em:description>
		</Description>
	</em:localized>
  </Description>
</RDF>
