<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>wikitheme@divwiki</em:id>
    <em:version>0.1</em:version>

    <!-- DivWiki -->
    <em:targetApplication>
      <Description>
        <em:id>divwiki@divwiki</em:id>
        <em:minVersion>0.1</em:minVersion>
        <em:maxVersion>2.0</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>WikiTheme</em:name>
    <em:description>Default skin/theme for DivWiki</em:description>

    <!-- Front End Integration Hooks (used by Theme Manager)-->
    <em:creator>Adrian Arroyo Calle</em:creator>
    <em:homepageURL>https://github.com/AdrianArroyoCalle/divwiki</em:homepageURL>

    <em:internalName>WikiTheme</em:internalName>
  </Description>
</RDF>
